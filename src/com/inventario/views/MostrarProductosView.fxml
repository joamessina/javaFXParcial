<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="465.0" prefWidth="474.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.inventario.controllers.MostrarProductosController">
   <children>
      <TableView fx:id="tableProductos" layoutX="14.0" layoutY="14.0" prefHeight="400.0" prefWidth="450.0">
         <columns>
            <TableColumn fx:id="colCodigo" text="Código" />
            <TableColumn fx:id="colTipo" text="Tipo" />
            <TableColumn fx:id="colNombre" text="Nombre" />
            <TableColumn fx:id="colPrecio" text="Precio" />
            <TableColumn fx:id="colStock" text="Stock" />
         </columns>
      </TableView>
      <Button fx:id="btnVolver" layoutX="180.0" layoutY="430.0" mnemonicParsing="false" onAction="#handleVolver" text="Volver al Menú Principal" />
   </children>
</AnchorPane>
